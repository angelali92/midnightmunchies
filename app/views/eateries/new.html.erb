
 
<%= form_for @eatery do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br/>
  <%= f.label :address %>:
  <%= f.text_field :address %><br/>
  <%= f.label :lat %>:
  <%= f.text_field :lat %><br/>
  <%= f.label :long %>:
  <%= f.text_field :long %><br/> 
  <%= f.label :phone_num %>:
  <%= f.text_field :phone_num %><br/> 
  <%= f.label :website %>:
  <%= f.text_field :website %><br/> 
  <%= f.label :rating %>:
