

<%= form_for @eatery do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br/>
  <%= f.label :address %>:
  <%= f.text_field :address %><br/>
  <%= f.label :lat_long %>:
  <%= f.text_field :lat_long %><br/>
  <%= f.label :phone_num %>:
  <%= f.text_field :phone_num %><br/>

  <%= f.submit %>
<% end %>

<%= link_to 'Back', eateries_path %>